<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <NeoContractName>$(AssemblyName)</NeoContractName>
    <NeoExpressBatchFile>../express.batch</NeoExpressBatchFile>
    <Nullable>enable</Nullable>
    <TargetFramework>net7.0</TargetFramework>
    <NeoTestVersion>3.5.17</NeoTestVersion>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="Chain.SmartContract.Framework" Version="3.5.0" />
    <PackageReference Include="Chain.BuildTasks" Version="$(NeoTestVersion)" PrivateAssets="all" />
  </ItemGroup>
</Project>